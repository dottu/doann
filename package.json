{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "dev": "npm run watch ./node_modules/.bin/babel-node ./src/bin/www",
    "prod": "npm run build && NODE_ENV=production ./node_modules/.bin/babel-node ./dist/bin/www",
    "migratedb": "npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "resetdb": "node src/database/config/createdb.js && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "build": "npm run clean && babel ./src --config-file ./.babelrc --out-dir dist",
    "clean": "rimraf dist",
    "watch": "nodemon --config ./nodemon.json --"
  },
  "dependencies": {
    "@babel/compat-data": "^7.10.1",
    "alert": "^5.0.10",
    "async-redis": "^1.1.7",
    "axios": "^0.19.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cli-highlight": "^2.1.4",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.0",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.0",
    "faker": "^5.1.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.31",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql": "^2.17.1",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.5.0",
    "pug": "^2.0.4",
    "redis": "^2.8.0",
    "rimraf": "^2.7.1",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.7",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "nodemon": "^1.19.4"
  }
}
